<Script>-- These actions are performed when the Next button is clicked.

--判断有没有安装过

hasInstalledDobby = Registry.DoesKeyExist(HKEY_CURRENT_USER, "Software\\WePush");

if hasInstalledDobby ~= false then

--读取安装路径
InstalledPath = Registry.GetValue(HKEY_CURRENT_USER, "Software\\WePush", "InstallPath", false);
SessionVar.Set("%AppFolder%", InstalledPath);
end

if hasInstalledDobby == false then

--读取安装路径
InstalledPath = "D:\\Program Files\\WePush";
SessionVar.Set("%AppFolder%", InstalledPath);
end

-- advance to the next screen
Screen.Next();
